{
  "author": {
    "name": "Cameron Hunter",
    "email": "hello@cameronhunter.co.uk",
    "url": "http://cameronhunter.co.uk"
  },
  "name": "alexa-lambda-skill",
  "description": "Create Alexa Skills for AWS Lambda, easily.",
  "version": "3.0.2",
  "main": "build/index.min.js",
  "jsnext:main": "src/index.js",
  "license": "MIT",
  "repository": "cameronhunter/alexa-lambda-skill",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build:dev": "rollup -c",
    "build:production": "NODE_ENV=production rollup -c",
    "build": "npm-run-all --parallel build:dev build:production",
    "pretest": "npm run build",
    "test": "ava --require babel-register --require babel-polyfill test/**/*test.js example/**/*test.js",
    "posttest": "npm run lint",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "lint": "eslint src/**/*.js example/**/*.js"
  },
  "dependencies": {
    "alexa-response": "^3.0.0",
    "bluebird": "^3.3.4",
    "object-values": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.5.2",
    "eslint": "^2.5.2",
    "isomorphic-fetch": "^2.2.1",
    "npm-run-all": "^1.5.3",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-inject": "^1.4.0",
    "rollup-plugin-uglify": "^0.3.1",
    "sinon": "^1.17.3"
  },
  "keywords": [
    "alexa",
    "echo",
    "skill",
    "amazon",
    "lambda"
  ],
  "ava": {
    "babel": "inherit"
  }
}
